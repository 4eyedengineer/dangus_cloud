# Local development secrets
# Copy to .env and fill in values

# Kubernetes token for k3s API access
# Generate with: kubectl create token dangus-backend -n dangus-cloud --duration=8760h
KUBERNETES_TOKEN=

# GitHub OAuth (optional for local dev)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Harbor credentials (optional - only needed if pushing images)
HARBOR_ROBOT_USER=
HARBOR_ROBOT_PASSWORD=
